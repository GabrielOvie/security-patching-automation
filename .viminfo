	---
	- name: Run CIS Compliance Scan using OpenSCAP on RHEL nodes
	  hosts: all
	  become: true
	  gather_facts: yes
	
	  vars:
	    cis_profile: xccdf_org.ssgproject.content_profile_cis
	    scap_base_path: /usr/share/xml/scap/ssg/content
	    remote_tmp: /tmp
	
	  tasks:
	    - name: Detect RHEL major version from facts
	      ansible.builtin.set_fact:
	        rhel_major: "{{ ansible_distribution_major_version }}"
	
	    - name: Set SCAP DS path based on RHEL version
	      ansible.builtin.set_fact:
	        scap_ds: "{{ scap_base_path }}/ssg-rhel{{ rhel_major }}-ds.xml"
	
	    - name: Install required SCAP package
	      ansible.builtin.package:
	        name: scap-security-guide
	        state: present
	
	    - name: Ensure SCAP DS file exists and is not empty
	      ansible.builtin.stat:
	        path: "{{ scap_ds }}"
	      register: ds_stat
	
	    - name: Fail if SCAP data stream is missing or empty
	      ansible.builtin.fail:
	        msg: "SCAP data stream is absent or empty: {{ scap_ds }}"
	      when: not ds_stat.stat.exists or ds_stat.stat.size == 0
	
	    - name: Run OpenSCAP CIS scan (HTML + XML)
	      ansible.builtin.command: >
	        oscap xccdf eval
	        --profile {{ cis_profile }}
	        --results {{ remote_tmp }}/results-{{ inventory_hostname }}.xml
	        --report {{ remote_tmp }}/report-{{ inventory_hostname }}.html
	        {{ scap_ds }}
	      register: oscap_eval
	      changed_when: false
	      failed_when: oscap_eval.rc not in [0, 2]  # 2 = noncompliant
	
	    - name: Create local reports folder
	      ansible.builtin.local_action:
	        module: file
	        path: "{{ playbook_dir }}/reports/{{ inventory_hostname }}"
