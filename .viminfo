	---
	- name: Run OpenSCAP CIS compliance scan on RHEL9 and fetch reports
	  hosts: all
	  become: yes
	  vars:
	    scap_file: /usr/share/xml/scap/ssg/content/ssg-rhel9-ds.xml
	    profile_id: xccdf_org.ssgproject.content_profile_cis
	    report_dir: "{{ playbook_dir }}/reports/{{ inventory_hostname }}"
	  tasks:
	    - name: Ensure scap-security-guide is installed
	      yum:
	        name: scap-security-guide
	        state: present
	
	    - name: Wait for scap data stream to exist
	      stat:
	        path: "{{ scap_file }}"
	      register: scap_stat
	      until: scap_stat.stat.exists
	      retries: 5
	      delay: 5
	
	    - name: Run OpenSCAP CIS scan (XML + HTML)
	      command: >
	        oscap xccdf eval
	        --profile "{{ profile_id }}"
	        --results /tmp/results.xml
	        --report /tmp/report.html
	        "{{ scap_file }}"
	      register: oscap_res
	      changed_when: false
	      failed_when: oscap_res.rc not in [0, 2]
	
	    - name: Create local report directory
	      file:
	        path: "{{ report_dir }}"
	        state: directory
	        mode: "0755"
	
	    - name: Fetch HTML and XML reports
	      fetch:
	        src: /tmp/report.html
	        dest: "{{ report_dir }}/report.html"
	        flat: yes
	
	    - name: Fetch XML results
	      fetch:
	        src: /tmp/results.xml
	        dest: "{{ report_dir }}/results.xml"
	        flat: yes
