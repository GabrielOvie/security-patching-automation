	---
	- name: CIS Compliance Scan and Report Collection for RHEL Fleet
	  hosts: all
	  become: true
	
	  pre_tasks:
	
	    - name: Get major RHEL version
	      command: rpm -q --qf "%{VERSION}\n" redhat-release
	      register: rhel_version_raw
	      changed_when: false
	
	    - name: Set RHEL major version
	      set_fact:
	        rhel_major_version: "{{ rhel_version_raw.stdout.split('.')[0] }}"
	
	    - name: Set SCAP data stream file path
	      set_fact:
	        scap_file: "/usr/share/xml/scap/ssg/content/ssg-rhel{{ rhel_major_version }}-ds.xml"
	
	  vars:
	    scap_package: scap-security-guide
	    profile: xccdf_org.ssgproject.content_profile_cis
	    output_dir: /tmp
	    report_files:
	      - report.html
	      - results.xml
	    local_report_dir: "{{ playbook_dir }}/security-patching-automation/reports/{{ inventory_hostname }}"
	
	  tasks:
	
	    - name: Ensure SCAP packages are installed
	      dnf:
	        name:
	          - "{{ scap_package }}"
	          - openscap-scanner
	        state: present
	
