	---
	- name: Run CIS scan on RHEL9 hosts
	  hosts: all
	  become: true
	  tasks:
	
	    - name: Ensure SCAP content package is installed
	      yum:
	        name: scap-security-guide
	        state: present
	
	    - name: Check for data stream file
	      stat:
	        path: /usr/share/xml/scap/ssg/content/ssg-rhel9-ds.xml
	      register: ssg_ds
	
	    - name: Fail if data stream missing or empty
	      fail:
	        msg: "SCAP data stream is absent or empty: /usr/share/xml/scap/ssg/content/ssg-rhel9-ds.xml"
	      when: not ssg_ds.stat.exists or ssg_ds.stat.size == 0
	
	    - name: Create local reports dir
	      file:
	        path: "{{ playbook_dir }}/reports/{{ inventory_hostname }}"
	        state: directory
	        owner: "{{ ansible_user }}"
	        mode: '0755'
	      delegate_to: localhost
	
	    - name: Run OpenSCAP CIS scan (HTML + ARF XML)
	      command: >
	        oscap xccdf eval
	          --profile xccdf_org.ssgproject.content_profile_cis
	          --results /tmp/{{ inventory_hostname }}-results.xml
	          --report /tmp/{{ inventory_hostname }}-report.html
	          /usr/share/xml/scap/ssg/content/ssg-rhel9-ds.xml
	      register: scan
	      failed_when:
	        - scan.rc != 0
	
	    - name: Fetch HTML and XML results
	      fetch:
	        src: "/tmp/{{ inventory_hostname }}-report.html"
	        dest: "reports/{{ inventory_hostname }}/"
	        flat: true
	      become: true
	
	    - name: Fetch ARF XML results
	      fetch:
	        src: "/tmp/{{ inventory_hostname }}-results.xml"
